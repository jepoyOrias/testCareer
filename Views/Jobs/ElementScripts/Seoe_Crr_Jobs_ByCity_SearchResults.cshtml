@{
    Seog_Crr_Log objLog = new Seog_Crr_Log();


}
@try
{
    <div class="margin-top30">
        <div class="flex-basis-fill">
            <h2 class="crr-h2-title margin-bottom20">Alabama</h2>
            <div class="crr-search-results">
                @for (var count = 0; count <= 4; count++)
                {
                    <a class="crr-search-result-div" href="-in-honolulu-ma">
                        <div class="crr-featuredjob-item-header margin-bottom20">
                            <div class="crr-search-item-title">Auburn</div>
                            <div class="crr-search-job-counts ">2 Jobs</div>
                        </div>
                    </a>
                }

            </div>
        </div>
        <div class="flex-basis-fill">
            <h2 class="crr-h2-title margin-bottom20 margin-top30">Alaska</h2>
            <div class="crr-search-results">
                @for (var count = 0; count <= 4; count++)
                {
                    <a class="crr-search-result-div" href="-in-honolulu-ma">
                        <div class="crr-featuredjob-item-header ">
                            <div class="crr-search-item-title">Auburn</div>
                            <div class="crr-search-job-counts">2 Jobs</div>
                        </div>
                    </a>
                }

            </div>
        </div>
    </div>

    <button type="button" class="btn btn-default crr-btn-blue margin-top30">View All Jobs</button>
}
catch (Exception ex)
{
    objLog.SalError("Seoe_Crr_Jobs_ByCity_SearchBox.cshtml", ex);
}