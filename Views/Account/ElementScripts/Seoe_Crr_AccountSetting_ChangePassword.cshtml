@{
    Seog_Crr_Log objLog = new Seog_Crr_Log();
}

@try
{
    <div class="crr-change-boder-style border-radius3 flex-div flex-align-item-start flex-justify-content-between padding20 margin-bottom15">
        <div class="flex-column crr-container">
            <div class="crr-privacy-security-title display-flex flex-justify-content-between">
                <div>
                    <div class="carr-h2 crr-text-primary">Change Password</div>
                    <div class="margin-top10 crr-text-primarydark text-size14">Update password settings for sally@gmail.com</div>
                </div>
                <div>
                    <img src="~/Images/down-icon.svg" class="crr-down-icon align-self-center" />
                    <img src="~/Images/up-icon.svg" class="crr-up-icon align-self-center display-none" />
                </div>
            </div>
            <div class="crr-container-main display-none">
                <div class="flex-column flex-justify-content-around">
                    <div class="crr-change-password-item display-flex flex-justify-content-end ">
                        <div class="crr-show-password align-self-end crr-text-blue text-size15">Show Passwords</div>
                    </div>
                    <div class="crr-change-password-item flex-div flex-align-item-center flex-justify-content-center flex-justify-content-between margin-bottom20">
                        <span class="crr-text-blue font-semibold">Current Password</span>
                        <input class="border-none border-radius4" id="crr-current-password" name="crr-current-password" type="text" value="" />
                    </div>
                    <div class="crr-change-password-item flex-div flex-align-item-center flex-justify-content-center flex-justify-content-between  margin-bottom20">
                        <span class="crr-text-blue font-semibold">
                            <span>New Password</span>
                        </span>
                        <input class="border-none border-radius4" id="crr-current-password" name="crr-current-password" type="text" value="" />
                    </div>
                    <div class="crr-change-password-item flex-div flex-align-item-center flex-justify-content-center flex-justify-content-between  margin-bottom20">
                        <span class="crr-text-blue font-semibold">Confirm New Password</span>
                        <input class="border-none border-radius4" id="crr-current-password" name="crr-current-password" type="text" value="" />
                    </div>
                    <div class="margin-top30 display-flex">
                        <button class="btn btn-default crr-container-click-here-cancel font-semibold crr-text-primary text-size16" type="button">
                            Cancel
                        </button>
                        <button type="button" class="btn btn-cta margin-left15">Save New Password</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
        }
        catch (Exception ex)
        {
        objLog.SalError("Seoe_Crr_Account_ChangePassword.cshtml", ex);
        }

