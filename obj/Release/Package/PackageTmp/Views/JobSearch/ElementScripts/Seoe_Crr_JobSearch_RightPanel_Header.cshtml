@{
    Seog_Crr_Log objLog = new Seog_Crr_Log();
    Seog_Crr_Data objData = new Seog_Crr_Data();
}

@try
{
    <div class="crr-sr-main-header-wrapper ">
        <div class="crr-sr-main-header padding-top20 padding-bottom20 margin-top30">
            <div class="crr-sr-haeader-left">
                <div class="crr-sr-header-title">
                    <div class="crr-search-item-title text-size24 crr-text-primary font-bold collapse-on-mobile padding-left10">Graphic Designer, Jr</div>
                    <div class="crr-sr-job-info margin-top10">
                        <div>
                            <a href="#">
                                <img class="crr-company-logo" data-src="holder.js/64x64" alt="64x64" src="data:image/svg+xml;base64," data-holder-rendered="true">
                            </a>
                        </div>
                        <div class="crr-company-desc flex-grow-1">
                            <div class="crr-search-item-title text-size18 crr-text-primary font-bold collapse-on-desktop">Graphic Designer, Jr</div>
                            <div class="crr-text-blue crr-search-info text-size18 margin-bottom10 margin-top10">Microsoft</div>
                            <div class="text-size18 crr-search-info">Redmond, WA | Full Time</div>
                        </div>
                        <div class="collapse-on-desktop">
                            <div class="text-size11 text-gray">25 applicants</div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="collapsable-on-scroll flex-basis-fill crr-sr-other-info margin-top10">
                <div class="crr-sr-job-title-info">
                    <div class="crr-sr-main-header-info">
                        <div>
                            <div class="text-size16 margin-top5">$45k-58k  | $35k-48k (estimate) <span class="icon-crr-info text-bluebright margin-left10 vertical-align-middle"></span></div>
                            <div class="margin-top5">Full Time | Entry Level | Multimedia Arts</div>
                        </div>
                    </div>
                    <div class="text-size13 text-gray margin-top10 margin-bottom10">
                        Posted 2 days ago
                    </div>
                </div>
            </div>
            <div class="crr-sr-haeader-right flex-column flex-justify-content-between flex-align-item-end">
                <div class="crr-sr-header-actions padding-left5 padding-right5">
                    <button class="btn btn-default btn-lg border-blue bg-white crr-text-primarydark">Send Me Jobs Like This <span class="icon-crr-ring crr-text-primary margin-left10"></span></button>
                    <button class="btn  btn-lg crr-btn-blue ">Apply <span class="icon-report-check collapse-on-desktop margin-left10"></span></button>
                </div>
            </div>
            <div class="crr-sr-header-save bg-grayfill collapsable-on-scroll flex-basis-fill">
                <a href="javascript:void(0)" class="text-size18"><span class="icon-crr-bookmark margin-right10"></span>Save</a>
            </div>
        </div>
    </div>
}
catch (Exception ex)
{
    objLog.SalError("Seoe_Crr_JobSearch_RightPanel_Header.cshtml", ex);
}
