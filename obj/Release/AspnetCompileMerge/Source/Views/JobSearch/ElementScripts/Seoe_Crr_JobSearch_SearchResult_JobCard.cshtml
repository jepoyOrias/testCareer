@{
    Seog_Crr_Log objLog = new Seog_Crr_Log();
    Seog_Crr_Data objData = new Seog_Crr_Data();
}

@try
{
    <div class="crr-sr-job-cards margin-bottom10">
        <div class="crr-sr-job-card margin-top15 active" data-title="Job-Title">
            <div class="display-flex flex-justify-content-between">
                <div class="crr-sr-job-info">
                    <div>
                        <a href="#">
                            <img class="crr-company-logo" data-src="holder.js/64x64" alt="64x64" src="data:image/svg+xml;base64," data-holder-rendered="true">
                        </a>
                    </div>
                    <div class="crr-company-desc flex-grow-1">
                        <div class="crr-search-item-title text-size20 crr-text-primary font-bold">Graphic Designer, Jr</div>
                        <div class="crr-text-blue margin-bottom10 margin-top20">Microsoft</div>
                        <div class="font-semibold">Redmond, WA | Full Time</div>
                        <div class="text-size14 margin-top10">$45k-58k  | $35k-48k (estimate) <span class="icon-crr-info text-bluebright margin-left10 vertical-align-middle"></span></div>
                    </div>
                </div>
                <div class="crr-job-info-action flex-column flex-justify-content-between">
                    <span class="icon-crr-bookmark crr-text-primary font-semibold"></span>
                    <span class="font-bold">2d</span>
                </div>
            </div>
        </div>
        @for (var count = 1; count <= 3; count++)
        {
            var dataTitle = "Job-title" + count;
            <div class="crr-sr-job-card margin-top15" data-title="@dataTitle">
                <div class="display-flex flex-justify-content-between">
                    <div class="crr-sr-job-info">
                        <div>
                            <a href="#">
                                <img class="crr-company-logo" data-src="holder.js/64x64" alt="64x64" src="data:image/svg+xml;base64," data-holder-rendered="true">
                            </a>
                        </div>
                        <div class="crr-company-desc flex-grow-1">
                            <div class="crr-search-item-title text-size20 crr-text-blue font-bold">Graphic Designer, Jr</div>
                            <div class="crr-text-blue margin-bottom10 margin-top20">Microsoft</div>
                            <div class="font-semibold">Redmond, WA | Full Time</div>
                            <div class="text-size14 margin-top10">$45k-58k  | $35k-48k (estimate) <span class="icon-crr-info text-bluebright margin-left10 vertical-align-middle"></span></div>
                        </div>
                    </div>
                    <div class="crr-job-info-action flex-column flex-justify-content-between">
                        <span class="icon-crr-bookmark crr-text-primary font-semibold"></span>
                        <span class="font-bold">2d</span>
                    </div>
                </div>
            </div>
        }
    </div>
}
catch (Exception ex)
{
    objLog.SalError("Seoe_Crr_JobSearch_SearchResult_JobCard.cshtml", ex);
}
