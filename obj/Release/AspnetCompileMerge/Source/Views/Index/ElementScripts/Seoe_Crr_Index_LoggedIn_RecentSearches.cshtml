@{
    Seog_Crr_Log objLog = new Seog_Crr_Log();
}

@try
{
    <div class="crr-recommended-jobs-layout margin-top30 flex-div flex-justify-content-center">
        @for (var count = 1; count <= 8; count++)
        {
            <div class="crr-job-list cursor-pointer border">
                <div>
                    <a href="#">
                        <img class="crr-company-logo" data-src="holder.js/64x64" alt="64x64" src="data:image/svg+xml;base64," data-holder-rendered="true">
                    </a>
                </div>
                <div class="crr-adv-desc crr-adv-flex-col flex-grow-1">
                    <div class="crr-item-title crr-text-blue font-bold text-size17">Account Coordinator</div>
                    <div>
                        <div class="crr-text-blue text-size15 margin-bottom10">University of Something</div>
                        <div class="text-size15 text-charcoal">Waltham, MA  |  Full Time</div>
                    </div>
                </div>
                <div class="crr-adv-actions crr-adv-flex-col flex-align-item-end">
                    <a href="#" class="crr-bookmark text-black"><span class="icon-crr-bookmark"></span></a>
                    <span class="text-size12">5 Days Ago</span>
                </div>
            </div>
        }
      
    </div>
    <div class="display-flex flex-justify-content-center margin-top30">
        <button class="btn btn-default  text-blue font-bold show-more-btn collapse-on-desktop" type="submit">Show more <span class="icon-crr-expandArrow text-blue text-size12 margin-left20 "></span></button>
    </div>
}
catch (Exception ex)
{
    objLog.SalError("Seoe_Crr_Index_LoggedIn_RecentSearches.cshtml", ex);
}