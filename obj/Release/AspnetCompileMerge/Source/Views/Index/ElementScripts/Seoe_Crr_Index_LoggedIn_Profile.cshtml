@{
    Seog_Crr_Log objLog = new Seog_Crr_Log();
}

@try
{
<div class="crr-user-profile-layout collapse-on-mobile">
    <div class="crr-user-profile-info margin-top20">
        <div class="text-center ">
            <div class="crr-profile">
                <div class="crr-user-profile-img">
                    <img src="~/Images/profile.jpg" />
                    <div class="crr-user-change-profile-btn"><span class="icon-add crr-text-primary text-size22 font-semibold"></span></div>
                </div>
                <div class="crr-user-profile-name crr-text-primarydark font-semibold text-size28 margin-top20">
                    Jim Walters
                </div>
                <div class="crr-user-edit-profile  margin-top15">
                    <button class="btn btn-default crr-border-primary crr-text-primary font-semibold text-size15" data-toggle="modal" data-target="#EditProfile"><div class="padding-left15 padding-right15">Edit Profile</div></button>
                </div>
            </div>
        </div>
        <ul class="list-group  crr-user-basic-info  margin-top10">
            <li class="list-group-item crr-text-primary font-semibold text-size16">
                <span class="icon-email margin-right10 crr-text-primary text-size20"></span>
                jim.walters@gmail.com
            </li>
            <li class="list-group-item crr-text-blue font-semibold text-size16">
                <span class="icon-crr-phone margin-right10 crr-text-blue text-size20"></span>
                999-999-9999
            </li>
            <li class="list-group-item crr-text-blue font-semibold text-size16">
                <span class="icon-crr-location margin-right10 crr-text-blue text-size20"></span>Boston, MA
            </li>
            <li role="separator" class="list-group-item divider padding0 margin10"></li>
            <li class="list-group-item crr-text-primary font-semibold text-size16">
                <span class="icon-crr-gear margin-right10 crr-text-primary text-size20"></span>
                My settings
            </li>
        </ul>
    </div>
</div>

    <div class="collapse-on-desktop crr-profile-mobile bg-grayfill">
        <div class="crr-user-profile-mobile-img">
            <img src="~/Images/profile.jpg" />
        </div>
        <div>
            <div class="crr-user-profile-name crr-text-primarydark text-size28">
                James Wilter
            </div>
            <a href="#" class="crr-text-blue text-size12">Profile</a>
        </div>
    </div>
}
catch (Exception ex)
{
    objLog.SalError("Seoe_Crr_Index_LoggedIn_Profile.cshtml", ex);
}