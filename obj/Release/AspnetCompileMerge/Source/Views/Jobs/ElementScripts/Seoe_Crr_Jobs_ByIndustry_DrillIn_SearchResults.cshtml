@{
    Seog_Crr_Log objLog = new Seog_Crr_Log();
}

@try
{
    <div class="crr-title-border text-size20 crr-black font-bold">
        Browse Job Listings in Construction (<span class="crr-text-blue">238 jobs</span>)
    </div>

    <div class="display-flex flex-justify-content-center margin-top30">
        <div class="crr-search-results">
            @for (var count = 0; count <= 20; count++)
            {
                <div class="crr-search-result-div  crr-search-result-drill-in">
                    <div class="display-flex flex-justify-content-between">
                        <div>
                            <a href="#">
                                <img class="crr-company-logo" data-src="holder.js/64x64" alt="64x64" src="data:image/svg+xml;base64," data-holder-rendered="true">
                            </a>
                        </div>
                        <div class="crr-job-info">
                            <div class="crr-search-item-title">Graphic Designer, Jr</div>
                            <div class="crr-text-blue margin-bottom10 margin-top20">{CompanyName}</div>
                            <div>Redmond, WA | Full Time</div>
                            <div class="text-size14 margin-top5">$45k-58k  | $35k-48k (estimate) <span class="icon-crr-info text-bluebright margin-left10 vertical-align-middle"></span></div>
                        </div>
                        <div class="crr-job-info-action">
                            <a href="javascript:void(0)" class="crr-bookmark" onclick="bookMark(this)">
                                <span class="icon-crr-bookmark text-black text-size18"></span>
                            </a>
                            <div class="text-size16 crr-text-primarydark">2d</div>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>

    <button type="button" class="btn btn-default crr-btn-blue margin-top30">View All 238 Jobs</button>
}
catch (Exception ex)
{
    objLog.SalError("Seoe_Crr_Jobs_ByIndustry_DrillIn_SearchResults.cshtml", ex);
}