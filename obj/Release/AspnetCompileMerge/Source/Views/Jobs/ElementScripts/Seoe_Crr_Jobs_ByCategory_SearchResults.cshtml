@{
    Seog_Crr_Log objLog = new Seog_Crr_Log();
}

@try
{
<div class="flex-column flex-justify-content-center" id="crr-jobs-wrapper">
    <div class="crr-search-results">
        @{

            string[] jobs = { "Creative Arts", "Another Category" };
            Random random = new Random();
        }

        @for (var count = 0; count <= 20; count++)
        {

            Int32 Index = random.Next(0, jobs.Length);
            var jobName = jobs[Index];

            //We need to create a url for each item here For now i will use a static url
            <a class="crr-search-result-div" href="-in-@(jobName.ToLower())-industry">
                <div class="crr-featuredjob-item-header">
                    <div class="crr-search-item-title">@jobName</div>
                    <div class="crr-search-job-counts margin-bottom10">2 Jobs</div>
                </div>
            </a>
        }
    </div>

    <div class="crr-jobs-pagination">
        <nav>
            <ul class="pagination">
                <li>
                    <a href="#" aria-label="Previous"><span aria-hidden="true">&laquo;</span></a>
                </li>
                <li class="active"><a href="#" class="margin-left10 margin-right10">1<span class="sr-only">(current)</span></a></li>
                <li><a href="#" class="margin-left10 margin-right10">2<span class="sr-only">(current)</span></a></li>
                <li><a href="#" class="margin-left10 margin-right10">3<span class="sr-only">(current)</span></a></li>
                <li><a href="#" class="margin-left10 margin-right10">4<span class="sr-only">(current)</span></a></li>
                <li><a href="#" class="margin-left10 margin-right10">5<span class="sr-only">(current)</span></a></li>
                <li>
                    <a href="#" aria-label="Next">
                        <span aria-hidden="true">&raquo;</span>
                    </a>
                </li>
            </ul>
        </nav>
    </div>
</div>
}
catch (Exception ex)
{
    objLog.SalError("Seoe_Crr_Jobs_ByCategory_SearchResults.cshtml", ex);
}