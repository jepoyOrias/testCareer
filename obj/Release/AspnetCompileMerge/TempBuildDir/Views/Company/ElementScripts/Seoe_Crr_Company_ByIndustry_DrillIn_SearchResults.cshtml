@{
    Seog_Crr_Log objLog = new Seog_Crr_Log();
}

@try
{
    <div class="flex-column flex-justify-content-center" id="crr-jobs-wrapper">
        <div class="crr-search-results">
            @{

                string[] companies = { "360 Concept", "Microsoft", "Google" };
                Random random = new Random();
            }

            @for (var count = 0; count <= 20; count++)
            {

                Int32 Index = random.Next(0, companies.Length);
                var companyName = companies[Index];

                //We need to create a url for each item here For now i will use a static url

                <div class="crr-search-result-div crr-search-result-drill-in">
                    <div class="crr-company-info">
                        <div>
                            <a href="#">
                                <img class="crr-company-logo" data-src="holder.js/64x64" alt="64x64" src="data:image/svg+xml;base64," data-holder-rendered="true">
                            </a>
                        </div>
                        <div class="crr-company-desc flex-grow-1">
                            <div class="crr-search-item-title">@companyName</div>
                            <p class="crr-search-job-counts">2 Jobs</p>
                            <p><span class="text-charcoal margin-right5 text-size14">INDUSTRY </span>Advertising/Marketing/PR</p>
                        </div>
                    </div>
                </div>

            }

        </div>
        <div class="crr-jobs-pagination">
            <nav>
                <ul class="pagination">
                    <li>
                        <a href="#" aria-label="Previous"><span aria-hidden="true">&laquo;</span></a>
                    </li>
                    <li class="active"><a href="#" class="margin-left10 margin-right10">1<span class="sr-only">(current)</span></a></li>
                    <li><a href="#" class="margin-left10 margin-right10">2<span class="sr-only">(current)</span></a></li>
                    <li><a href="#" class="margin-left10 margin-right10">3<span class="sr-only">(current)</span></a></li>
                    <li><a href="#" class="margin-left10 margin-right10">4<span class="sr-only">(current)</span></a></li>
                    <li><a href="#" class="margin-left10 margin-right10">5<span class="sr-only">(current)</span></a></li>
                    <li>
                        <a href="#" aria-label="Next">
                            <span aria-hidden="true">&raquo;</span>
                        </a>
                    </li>
                </ul>
            </nav>
        </div>
    </div>
}
catch (Exception ex)
{
    objLog.SalError("Seoe_Crr_Company_ByIndustry_DrillIn_SearchResults.cshtml", ex);
}